script(type="text/template",id="reclip-view-template")
  div.masker_layer
  section.small_pop
    h2.subject_put.
      <% if(count){ %>
      您将收藏<%= count%>条数据
      <% } else { %>
      收
      <% } %>
    ul.pop_body
      li.error.
        <%= typeof error != "undefined" && error ? error : "" %>
      li.tag_box
      li.input_text
        input(type="text")
      li.input_textarea
        textarea
      li.input_checkbox
        label
          input(type="checkbox")
          | 不公开
      li.btn_group.clearfix
        input.verify(type="button",value="确定")
        input.cancel(type="button",value="取消")